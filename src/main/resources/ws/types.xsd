<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
	xmlns="http://www.example.org/huge64-types" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/huge64-types">

    <xsd:complexType name="HugeFile">
    	<xsd:sequence>
    		<xsd:element name="fileName" type="xsd:string"></xsd:element>
    		<xsd:element name="xml" type="xsd:base64Binary"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UploadStatus">
    	<xsd:sequence>
    		<xsd:element name="status" type="xsd:string"></xsd:element>
    		<xsd:element name="errorDescription" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
</xsd:schema>